{
  "name": "ngx-daterangepicker-material",
  "version": "3.0.2",
  "description": "Angular 2+ date range picker (with material design theme)",
  "keywords": [
    "angular2",
    "angular4",
    "angular5",
    "angular6",
    "ng2",
    "daterangepicker",
    "material",
    "component",
    "directive",
    "daterange",
    "date",
    "moment",
    "picker"
  ],
  "homepage": "https://github.com/fetrarij/ngx-daterangepicker-material",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "schematics": "./schematics/collection.json",
  "scripts": {
    "ng": "ng",
    "start": "npm run build-themes && ng serve",
    "deploy": "ng deploy",
    "demo:build": "ng build --prod",
    "test": "./node_modules/.bin/ng test --watch=false",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "ngc": "./node_modules/.bin/ngc -p tsconfig.json",
    "gh-pages": "ng build --prod --base-href=\"/ngx-daterangepicker-material/\" && ngh --dir demo/dist",
    "build": "ng-packagr -p package.json && npm run build-themes && npm run bundle-styles",
    "build-themes": "bash ./tools/build-themes.sh && cp -R ./tools/prebuilt-themes/ dist/",
    "bundle-styles": "scss-bundle -c scss-bundle.config.json",
    "build:schematics": "tsc -p ./tsconfig.schematics.json",
    "copy:schematics": "cpr schematics/dist dist/schematics && cp ./schematics/collection.json ./dist/schematics && cp ./schematics/ng-add/schema.json ./dist/schematics/ng-add",
    "postbuild": "npm run build:schematics && npm run copy:schematics",
    "prettier": "prettier --write '**/*.{html,js,ts,css,scss,less,md,json}'",
    "prettier-check": "prettier --list-different '**/*.{html,js,ts,css,scss,less,md,json}'"
  },
  "author": "Fetrarijaona R.",
  "repository": {
    "type": "git",
    "url": "https://github.com/fetrarij/ngx-daterangepicker-material.git"
  },
  "peerDependencies": {
    "moment": "^2.27.0",
    "@angular/common": ">=10.0.0",
    "@angular/core": ">=10.0.0",
    "@angular/material": ">=10.0.0",
    "@angular-devkit/schematics": "^10.0.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1000.1",
    "@angular/animations": "^10.0.2",
    "@angular/cdk": "^10.0.1",
    "@angular/cli": "10.0.1",
    "@angular/common": "^10.0.2",
    "@angular/compiler": "^10.0.2",
    "@angular/compiler-cli": "^10.0.2",
    "@angular/core": "^10.0.2",
    "@angular/forms": "^10.0.2",
    "@angular/material": "^10.0.1",
    "@angular/platform-browser": "^10.0.2",
    "@angular/platform-browser-dynamic": "^10.0.2",
    "@angular/router": "^10.0.2",
    "@types/jasmine": "~3.5.11",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^14.0.14",
    "angular-cli-ghpages": "^0.6.2",
    "codelyzer": "^6.0.0",
    "core-js": "^3.6.5",
    "cpr": "^3.0.1",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~5.0.2",
    "karma": "~5.1.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~3.3.0",
    "karma-jasmine-html-reporter": "^1.5.4",
    "moment": "^2.27.0",
    "ng-packagr": "^9.1.1",
    "ngx-highlightjs": "^4.1.0-beta",
    "node-sass": "^4.14.1",
    "prettier": "^2.0.5",
    "protractor": "~7.0.0",
    "rxjs": "^6.6.0",
    "scss-bundle": "^3.1.2",
    "ts-node": "~8.10.2",
    "tslib": "^2.0.0",
    "tslint": "~6.1.0",
    "typescript": "~3.9.6",
    "zone.js": "~0.10.2"
  },
  "$schema": "./node_modules/ng-packagr/package.schema.json",
  "ngPackage": {
    "lib": {
      "entryFile": "src/daterangepicker/index.ts",
      "umdModuleIds": {
        "moment": "moment"
      }
    },
    "dest": "dist"
  },
  "dependencies": {
  }
}
